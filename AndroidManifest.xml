<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chaturvedi.kbc_prakalpa"
    android:versionCode="001"
    android:versionName="1.0.1" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity 
            android:name="com.chaturvedi.kbc_prakalpa.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name="com.chaturvedi.kbc_prakalpa.MainActivity" 
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.chaturvedi.kbc_prakalpa.Question01Activity"></activity>
        <activity android:name="com.chaturvedi.kbc_prakalpa.Question01AnsweredActivity"></activity>
        <activity android:name="com.chaturvedi.kbc_prakalpa.Question01FailedActivity"></activity>
        <activity android:name="com.chaturvedi.kbc_prakalpa.Question01TimeoutActivity"></activity>
        <service android:enabled="true" android:name=".MediaPlayerService"></service>
    </application>

</manifest>
